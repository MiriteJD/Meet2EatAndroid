<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dreher.meet2eat">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/DHBW">
        <supports-screens
            android:anyDensity="true"
            android:largeScreens="true"
            android:smallScreens="true" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Registry"
            android:label="@string/title_activity_registry"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NeuesEvent"
            android:label="@string/title_activity_neues_event"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Event"
            android:label="@string/title_activity_event" />
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home" />
        <activity
            android:name=".Navi"
            android:label="@string/title_activity_navi" />
        <activity
            android:name=".Profil"
            android:label="@string/title_activity_profil" />
        <activity android:name=".Einstellungen"></activity>
    </application>

</manifest>
